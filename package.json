{
	"name": "BlacklakeTranslate",
	"displayName": "BlacklakeTranslate",
	"description": "Translate Extension For Blacklake",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.46.0"
	},
	"publisher": "BlacklakeTechWeb",
	"repository": {
		"type": "git",
		"url": "https://github.com/BlacklakeTechWeb/BlacklakeTranslate.git"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.BLi18n",
		"onCommand:extension.BLFormattedMessage"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.BLi18n",
				"title": "BLTranslate: BLi18n"
			},
			{
				"command": "extension.BLFormattedMessage",
				"title": "BLTranslate: BLFormattedMessage"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "extension.BLi18n"
				},
				{
					"command": "extension.BLFormattedMessage"
				}
			]
		},
		"keybindings": [
			{
				"command": "extension.BLFormattedMessage",
				"key": "ctrl+i",
				"mac": "cmd+i",
				"when": "editorTextFocus"
			},
			{
				"command": "extension.BLi18n",
				"key": "cmd+shift+i",
				"mac": "cmd+shift+i",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"eslint": "^7.9.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"@vitalets/google-translate-api": "^4.0.0"
	}
}